            fx:controller="com.example.suivipatientjavafx.Controller.RendezVousController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Liste des rendez-vous" />

        <TableView fx:id="tableViewRendezVous" layoutX="100.0" layoutY="100.0" prefHeight="200.0" prefWidth="376.0">
            <columns>
                <TableColumn fx:id="col1" text="C1" prefWidth="188.0"/>
                <TableColumn fx:id="col2" text="C2" prefWidth="188.0"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
